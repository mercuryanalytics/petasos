{
  "name": "mercury-analytics-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "auth0-js": "^9.13.2",
    "date-fns": "^2.11.0",
    "final-form": "^4.18.7",
    "react": "^16.12.0",
    "react-datepicker": "^2.14.0",
    "react-dom": "^16.12.0",
    "react-final-form-hooks": "^2.0.2",
    "react-icons": "^3.10.0",
    "react-images-uploading": "^2.1.1",
    "react-redux": "^7.2.0",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-tt": "^1.1.8",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "simplebar-react": "^2.1.0",
    "url-parse": "^1.4.7"
  },
  "scripts": {
    "start": "PORT=3004 REACT_APP_VERSION=$(git rev-parse HEAD) react-scripts start",
    "build": "REACT_APP_VERSION=$(git rev-parse HEAD) react-scripts build",
    "test": "REACT_APP_VERSION=$(git rev-parse HEAD) react-scripts test",
    "eject": "react-scripts eject",
    "sync": "aws s3 sync build/ s3://$BUCKET_NAME",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $DISTRIBUTION_ID --paths \"/*\"",
    "s3deploy": "npm --prefix $CODEBUILD_SRC_DIR/ui/ run-script build && npm --prefix $CODEBUILD_SRC_DIR/ui/ run-script sync && npm --prefix $CODEBUILD_SRC_DIR/ui/ run-script invalidate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
